<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta http-equiv="Content-Language" content="en">
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<title>{{ title }}</title>
	<meta name="viewport"
		content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no, shrink-to-fit=no" />
	<meta name="description" content="{{ description }}">
	<!-- Disable tap highlight on IE -->
	<meta name="msapplication-tap-highlight" content="no">
  <link rel="stylesheet" href="./assets/styles/frontend.css">
	<script type="text/javascript" src="https://assets.inplayer.com/paywall/latest/paywall.min.js"></script>
	<script src="https://unpkg.com/masonry-layout@4/dist/masonry.pkgd.min.js"></script>
</head>
<body>
	<main role="main">
		{{> components/navigation
					prefix=global.prefix
					data=global.base }}

		{{> components/top-bar
					closed=heroClosed
					prefix=global.prefix }}

		{{> hero/11-01-2020
					closed=heroClosed
					prefix=global.prefix }}

		<div id="yield">
			{{#if social}}
			{{else}}
				{{> components/fix-social
						prefix=global.prefix }}
			{{/if}}

			{{> layouts/header
						nav=global.base.navigation_open
						prefix=global.prefix }}

			{{> @partial-block }}

			{{> components/subscribe
						data=global.home.subscribe
						prefix=global.prefix }}

		</div>
		{{> layouts/footer
					prefix=global.prefix }}

		{{> components/overlay
					prefix=global.prefix }}
	</main>
  <script src="./assets/scripts/frontend.js"></script>
</body>

</html>
